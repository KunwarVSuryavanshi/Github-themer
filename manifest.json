{
  "manifest_version": 2,
  "name": "Github Themer?",
  "version": "0.69",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ], 
      "js": [
        "content.js"
      ] 
    }
  ],
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "permissions": ["https://github.com/*", "tabs"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Hey Boy"
  }
}